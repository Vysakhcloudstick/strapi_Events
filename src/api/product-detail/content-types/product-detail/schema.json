{
  "kind": "collectionType",
  "collectionName": "product_details",
  "info": {
    "singularName": "product-detail",
    "pluralName": "product-details",
    "displayName": "productData"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "availability": {
      "type": "string"
    },
    "rating": {
      "type": "decimal"
    },
    "review": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "rentalDetail": {
      "type": "richtext"
    },
    "quantity": {
      "type": "integer"
    },
    "imageThumbnail": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "detailDescription": {
      "type": "text"
    },
    "descriptionFeatures": {
      "type": "richtext"
    },
    "RentalTerms": {
      "type": "richtext"
    },
    "specifications": {
      "type": "component",
      "component": "shared.specifications",
      "repeatable": false
    },
    "description": {
      "type": "string"
    }
  }
